<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.n52.eventing.wv.model.Phenomenon" table="phenomenon" mutable="true">
        <meta attribute="class-description">
            This class contains the phenomenon details.
        </meta>
        <id name="id" type="int" column="pkid">
            <generator class="identity"/>
        </id>
        <property name="phenomenonId" column="phenomenon_id" type="string"/>
        <property name="name" column="phenomenon_name" type="string"/>
    </class>
</hibernate-mapping>
